  	    <li><%= link_to 'About', pages_home_path(@pages_home, anchor: "about") %></li>
  	    <li><%= link_to 'Recents Works', pages_works_path %></li>
  	    <li><%= link_to 'Writings', pages_writings_path %></li>
  	    <li><%= link_to 'Travel', pages_world_path %></li>
  	    <li><%= link_to 'Contact', pages_contact_path %></li>
  	  </ul>
  	  <hr class="dark" />
  	  <div>
  	    <ul class="social">
  	      <li><a href="https://github.com/marcusbey" target="_blank"><i class="fa fa-github"></i></a></li>
  	      <li><a href="https://www.linkedin.com/in/romainboboe/" target="_blank"><i class="fa fa-linkedin"></i></a></li>
